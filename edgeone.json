{
  "name": "api-eallion-com",
  "nodeVersion": "22.11.0",
  "routes": [
    {
      "pattern": "/gts/*",
      "script": "functions/gts/[[path]].js",
      "methods": ["GET", "POST", "PUT", "HEAD"]
    }
  ],
  "redirects": [
    {
      "source": "/memos/memos.json*",
      "destination": "/memos"
    }
  ],
  "headers": [
      {
        "source": "/*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Cache-Control",
            "value": "no-store, max-age=0, s-maxage=0, must-revalidate"
          }
        ]
      },
      {
        "source": "/gravatar*",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/webp"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=604800"
          }
        ]
      },
      {
        "source": "/mastodon*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, max-age=0, s-maxage=0, must-revalidate"
          }
        ]
      },
      {
        "source": "/memos*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, max-age=0, s-maxage=0, must-revalidate"
          }
        ]
      },
      {
        "source": "/ip*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, max-age=0, s-maxage=0, must-revalidate"
          }
        ]
      },
      {
        "source": "/geo*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, max-age=0, s-maxage=0, must-revalidate"
          }
        ]
      },
      {
        "source": "/favicon*",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/png"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=604800"
          }
        ]
      },
      {
        "source": "/og*",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/png"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      },
      {
        "source": "/bing*",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          }
        ]
      }
    ],
  "cache": [
    {
      "source": "/bing*",
      "cacheTtl": 3600
    },
    {
      "source": "/favicon*",
      "cacheTtl": 604800
    },
    {
      "source": "/og*",
      "cacheTtl": 31536000
    },
    {
      "source": "/gravatar*",
      "cacheTtl": 604800
    },
    {
      "source": "/memos*",
      "cacheTtl": 0
    },
    {
      "source": "/mastodon*",
      "cacheTtl": 0
    },
    {
      "source": "/ip*",
      "cacheTtl": 0
    },
    {
      "source": "/geo*",
      "cacheTtl": 0
    }
  ]
}
